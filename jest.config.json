{
    "verbose": true,
    "testRegex": "(/src/.*|(\\.|/))test.js?$",
    "transformIgnorePatterns": [
      "node_modules/(?!@jaxolotl)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/.git/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  }